entrypoint = "README.md"

modules = ["python-3.12"] 

[run]
command = "streamlit run streamlit_app.py"
args = ["--server.port", "8501"] 

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
# Simplified deployment command
run = ["streamlit", "run", "streamlit_app.py"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 8501
externalPort = 80